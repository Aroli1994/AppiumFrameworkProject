<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<groups>
		<run>
			<include name="Smoke" />
		</run>
	</groups>
	<listeners>
		<listener class-name="org.rahulshettyacademy.TestUtils.Listeners" />
	</listeners>
	<test thread-count="5" name="End to end framework test">
		<classes>
			<class name="org.rahulshettyacademy.eCommerce_tc_4_Hybrid" />
		</classes>
	</test> <!--
	Test -->

	<test thread-count="5" name="Error validation test">
		<classes>
			<class name="org.rahulshettyacademy.eCommerce_tc_1" />
		</classes>
	</test> <!--
	Test -->

	<test thread-count="5" name="IOS test">
		<classes>
			<class name="org.rahulshettyacademy.IOSBasics" />
		</classes>
	</test> <!--
	Test -->
</suite> <!--
Suite -->

